{
  "manifest_version": 3,
  "name": "hello4",
  "description": "Def Hack 2024",
  "version": "1.0",
  "icons": {},
  "action": {
    "default_popup": "popup.html"
  },
  "permissions": ["activeTab", "scripting"],
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Ctrl+B",
        "mac": "Command+B"
      }
    }
  },
  "background": {
    "service_worker": "background.js",
    "type":"module"
  }
}
