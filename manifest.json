{
  "manifest_version": 3,
  "name": "fact-check",
  "description": "Def Hack 2024",
  "version": "1.0",
  "icons": {
    "16": "images/Fact16.png",
    "32": "images/Fact32.png",
    "48": "images/Fact48.png",
    "128": "images/Fact128.png"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "permissions": ["activeTab", "scripting"],
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Ctrl+B",
        "mac": "Command+B"
      }
    }
  },
  "background": {
    "service_worker": "background.js",
    "type":"module"
  }
}
