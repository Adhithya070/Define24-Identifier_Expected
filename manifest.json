{
  "manifest_version": 3,
  "name": "FactCheck - Define2024",
  "description": "Scans the contents of a webpage and checks for misinformation",
  "version": "1.0",
  "icons": {
    "16": "images/Fact16.png",
    "32": "images/Fact32.png",
    "48": "images/Fact48.png",
    "128": "images/Fact128.png"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "permissions": ["activeTab", "scripting"],

  "background": {
    "service_worker": "background.js",
    "type":"module"
  }
}
